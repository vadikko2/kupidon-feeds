# Postgres (asyncpg)
POSTGRES_HOSTNAME=localhost
POSTGRES_PORT=5432
POSTGRES_DATABASE=feeds
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_POOL_MIN_SIZE=5
POSTGRES_POOL_MAX_SIZE=20
